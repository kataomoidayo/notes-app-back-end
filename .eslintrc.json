{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off"
    },
    //tambah agar import tidak error
    "import/no-extraneous-dependencies": [
        "error",
        {
            "devDependencies": [
                "**/*.test.ts",
                "**/*.test.tsx"
            ]
        }
    ]
}